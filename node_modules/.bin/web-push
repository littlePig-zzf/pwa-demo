#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../_web-push@3.3.0@web-push/src/cli.js" "$@"
  ret=$?
else 
  node  "$basedir/../_web-push@3.3.0@web-push/src/cli.js" "$@"
  ret=$?
fi
exit $ret
